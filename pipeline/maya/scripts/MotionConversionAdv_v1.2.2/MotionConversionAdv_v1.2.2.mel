global proc do_sizectrl(float $cvsize){
	
	string $firstsl[] = `ls -sl`;	
	select -hi; string $curvesA[] = `ls -sl -type "nurbsCurve"`;select -cl  ;
	select $curvesA;
	int $cursiz = `size $curvesA`;
	int $szdo;
	
	for ($szdo = 0; $szdo < $cursiz; $szdo++){
		
		string $ifrotx;
		int $ifts;
		$ifrotx = `match "RootX_MShape" $curvesA[$szdo]`;
		$ifts = `size $ifrotx`;
		
		if ($ifts>0){
			select -d $curvesA[$szdo];
			
		}
		
	}
	
	select -d RootX_MShape RootX_MShape1  RootX_MShape2  RootX_MShape3;
	string $curves[] = `ls -sl`; 
	select -cl ;
	int $sizecurves = `size $curves`;
	while ($sizecurves > 0){  
	int $num;
	$num = `size($curves[$sizecurves - 1])`;
	$num -=5;
	string $newstringAs = `substring $curves[$sizecurves - 1] 1 $num`;
	
	float $worldps[] = `xform -worldSpace -query -translation ($newstringAs + ".scalePivot")`;
	string $infoNode = `createNode curveInfo`;
	connectAttr ($newstringAs +".worldSpace")  ($infoNode+".inputCurve");
	int $degree = eval("getAttr " + $newstringAs + ".degree");
	int $spans = eval("getAttr "+$newstringAs+".spans");
	int $numCVs = $spans + $degree;
	int $fst = $numCVs - 1; 			
	select ($newstringAs + ".cv[0" + ":" + $fst + "]");
	scale -r -p ($worldps[0] + "cm") ($worldps[1] + "cm") ($worldps[2] + "cm") $cvsize $cvsize $cvsize ; 
	$sizecurves = $sizecurves - 1;
	}
	
	select -r $firstsl;

}

