global proc as_MakeController(string $Target_CSK, string $ID_Ctr, string $ot_Joint){
	
	int $SlectID = `text -q -l $ID_Ctr`;
	string $TargetJoint = `textField -q -tx $Target_CSK`; 
	int $sizeTarget = `size $TargetJoint`;
	
	
	if ($SlectID==0){
		
		print "Please selected your Type!";
		
	}
	
	else if ($sizeTarget==0){
		
		print "Please selected your target joint!";
		
	}
	
	else {
	
		if ($SlectID==1){
		
		//print "BIP";
		as_BipToAdvController($TargetJoint,$ot_Joint);
		
	}
		else if ($SlectID==2){
		
		//print "ue4";
		as_UE4ToAdvController($TargetJoint);
		
		}
		
	//	print "Succese!!!";
	}
	
}


global proc as_BakeAdvFK(string $Target_SK, string $Adv_SK, string $ID_anim){
	
	int $SlectID = `text -q -l $ID_anim`;
	string $TargetJoint = `textField -q -tx $Target_SK`; 
	string $AdvMain = `textField -q -tx $Adv_SK`; 
	int $sizeTarget = `size $TargetJoint`;
	int $sizeAdvmain = `size $AdvMain`;
	
	
	 if ($SlectID==0){
		
		print "Please selected your Conversion type!";

	 }
	
	else if ($sizeTarget==0){
		
		print "Please selected your target joint!";
		
	}
	else if ($sizeAdvmain==0){
		
		print "Please selected your AdvMain!";
		
	}
	
	else {
		
		if ($SlectID==1){ 
		
		
		if (`objExists B2A_FK_Animation`){
			
			animLayer -edit -weight 0 B2A_FK_Animation;
			
			delete B2A_FK_Animation;
		}

		
		as_BipToTpose($TargetJoint);
		
		as_advToBipTpose($AdvMain, $TargetJoint);
		
		as_BakeFK_Bip($AdvMain, $TargetJoint);
		
		}
		else if ($SlectID==2){
			
			if (`objExists B2A_FK_Animation`){
			
			animLayer -edit -weight 0 B2A_FK_Animation;
			
			delete B2A_FK_Animation;
		}
			
			as_MixamoToTpose($TargetJoint);
			
			as_AdvToMixamoTpose($AdvMain, "Hips");
			
			as_BakeFK_Mixamo($AdvMain, "Hips");
		}
		
		print "Succese!!!";
	}
	
}


global proc as_BakeAdvIK(string $Adv_SK){
	
	string $AdvMain = `textField -q -tx $Adv_SK`; 
	int $sizeAdvmain = `size $AdvMain`;
	
	if ($sizeAdvmain==0){
		print "Please Select your Adv main control!";
	}
	
	else {
		
		as_BakeIK($AdvMain);
		
		print "Succese!!!";
		
	}
	
}


